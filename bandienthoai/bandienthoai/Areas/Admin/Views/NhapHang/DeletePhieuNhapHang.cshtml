@model bandienthoai.Areas.Admin.Models.EF.PHIEUNHAPHANG

@{
    Layout = null;
}

<div id="deleteModal" class="modal" data-easein="slideDownIn" tabindex="-1" role="dialog" aria-labelledby="costumModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <h1>Bạn muốn xóa!</h1>
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                    ×
                </button>

            </div>
            <form>
                <div class="modal-body d-flex">
                    <div class="form-group">
                        <label for="exampleInputEmail1">@Model.SOPHIEUNHAPHANG</label>
                    </div>
                    <div class="form-group">
                        <label for="exampleInputEmail1">@Model.NGAYLAP_PHIEUNHAPHANG</label>

                    </div>
                    
                </div>
                <div class="modal-footer">
                    <button class="btn btn-default" data-dismiss="modal" aria-hidden="true">
                        Đóng
                    </button>
                    @using (Html.BeginForm())
    {
                    @Html.AntiForgeryToken()
                    <button type="submit" class="btn btn-primary">
                        Lưu
                    </button>

}

                </div>
            </form>
        </div>
    </div>
</div>

